{
    "manifest_version": 3,
    "name": "Scripting Example",
    "version": "1.0",
    "permissions": [
      "scripting",
      "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["test.js"]
        }
    ],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    }
  }